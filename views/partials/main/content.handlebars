

<main class="quest-main mdl-layout__content">
<div class="quest-page-space">
         
        </div>
    <div class="quest-container mdl-grid">
        
        {{#each questionnaires}}
        <div class="quest-card-square mdl-card mdl-shadow--2dp">
            <div class="mdl-card__title mdl-card--expand express">
                <h2 class="mdl-card__title-text mdl-card__title">{{ name }}</h2>
            </div>
            <div class="mdl-card__supporting-text">
                {{ description }}
            </div>
            <div class="mdl-card__actions mdl-card--border">
                <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect"  data-id={{ id }}>
                    Do It!
                </a>
            </div>
        </div>
        {{/each}}

